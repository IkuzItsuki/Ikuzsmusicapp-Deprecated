<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/top"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="402dp"
            app:layout_constraintBottom_toTopOf="@+id/songie"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView8"
            app:layout_constraintEnd_toEndOf="@+id/top"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/top"
            android:layout_marginEnd="32dp"
            android:alpha="0"
            app:layout_constraintTop_toTopOf="@+id/top"
            android:layout_marginTop="25dp"
            app:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:id="@+id/imageView9"
            app:layout_constraintEnd_toStartOf="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/imageView10"
            android:layout_marginEnd="24dp"
            android:alpha="0"
            app:layout_constraintTop_toTopOf="@+id/imageView10" />
        <Constraint
            android:id="@+id/imageView10"
            app:layout_constraintEnd_toStartOf="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/imageView8"
            android:layout_marginEnd="24dp"
            android:alpha="0"
            app:layout_constraintTop_toTopOf="@+id/imageView8" />
        <Constraint
            android:id="@+id/progressBar"
            app:layout_constraintEnd_toEndOf="@+id/top"
            android:layout_width="0dp"
            android:layout_height="4dp"
            android:alpha="0"
            app:layout_constraintTop_toTopOf="@id/top"
            app:layout_constraintStart_toStartOf="@+id/top" />
        <Constraint
            android:id="@+id/backArrow"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:rotation="-90"
            android:layout_marginStart="28dp"
            app:layout_constraintTop_toTopOf="@+id/top"
            app:layout_constraintStart_toStartOf="@+id/top"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/textView12"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="70dp"
            app:layout_constraintTop_toTopOf="@+id/top"
            app:layout_constraintStart_toStartOf="@+id/top"
            android:layout_marginTop="15dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView9"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView14"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="70dp"
            app:layout_constraintStart_toStartOf="@+id/top"
            app:layout_constraintEnd_toStartOf="@+id/imageView9"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            android:alpha="0"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp" />
        <Constraint
            android:id="@+id/playerArt"
            app:layout_constraintEnd_toEndOf="@+id/top"
            android:layout_width="330dp"
            android:layout_height="330dp"
            app:layout_constraintBottom_toBottomOf="@id/top"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintTop_toTopOf="@id/top"
            app:layout_constraintStart_toStartOf="@+id/top" />
        <Constraint
            android:id="@+id/songie"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/top"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/playerSong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            app:layout_constraintTop_toTopOf="@+id/songie"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/playerArtist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/playerSong"
            android:layout_marginStart="28dp"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:layout_marginTop="8dp" />
        <Constraint
            android:id="@+id/playerMore"
            app:layout_constraintEnd_toEndOf="@+id/songie"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginEnd="32dp"
            app:layout_constraintTop_toTopOf="@+id/songie"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/seekBar"
            app:layout_constraintEnd_toStartOf="@+id/playerDura"
            app:layout_constraintStart_toEndOf="@+id/playerDuration"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="@+id/songie"
            android:layout_marginTop="208dp" />
        <Constraint
            android:id="@+id/playerDuration"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/playerArtist"
            android:layout_marginStart="28dp"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:layout_marginTop="109dp" />
        <Constraint
            android:id="@+id/playerDura"
            app:layout_constraintEnd_toEndOf="@+id/songie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            app:layout_constraintTop_toBottomOf="@+id/playerMore"
            android:layout_marginTop="136dp" />
        <Constraint
            android:id="@+id/playerBack"
            android:layout_width="48dp"
            android:layout_height="48dp"
            app:layout_constraintBottom_toBottomOf="@+id/songie"
            app:layout_constraintTop_toBottomOf="@+id/seekBar"
            android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="@+id/songie" />
        <Constraint
            android:id="@+id/playerPause"
            app:layout_constraintEnd_toStartOf="@+id/playerNext"
            app:layout_constraintStart_toEndOf="@+id/playerBack"
            android:layout_width="48dp"
            android:layout_height="48dp"
            app:layout_constraintBottom_toBottomOf="@+id/songie"
            app:layout_constraintTop_toBottomOf="@+id/seekBar" />
        <Constraint
            android:id="@+id/playerNext"
            app:layout_constraintEnd_toEndOf="@+id/songie"
            android:layout_width="48dp"
            android:layout_height="48dp"
            app:layout_constraintBottom_toBottomOf="@+id/songie"
            android:layout_marginEnd="80dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBar" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/songie"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        <Constraint
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="75dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/top"
            app:layout_constraintBottom_toTopOf="@+id/songie"
            app:layout_constraintVertical_bias="1.0" />
        <Constraint
            android:id="@+id/playerArt"
            app:layout_constraintEnd_toEndOf="@+id/top"
            android:layout_width="55dp"
            android:layout_height="55dp"
            app:layout_constraintBottom_toBottomOf="@id/top"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintTop_toTopOf="@id/top"
            app:layout_constraintStart_toStartOf="@+id/top"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:id="@+id/backArrow"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:rotation="-90"
            android:layout_marginStart="28dp"
            app:layout_constraintTop_toTopOf="@+id/top"
            app:layout_constraintStart_toStartOf="@+id/top"
            android:layout_marginTop="16dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView12"
            app:layout_constraintEnd_toStartOf="@+id/imageView9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@id/textView14"
            android:layout_marginEnd="16dp"
            android:alpha="1"
            android:layout_marginStart="70dp"
            app:layout_constraintTop_toTopOf="@+id/top"
            app:layout_constraintStart_toStartOf="@+id/top"
            android:layout_marginTop="15dp" />
        <Constraint
            android:id="@+id/textView14"
            app:layout_constraintEnd_toStartOf="@+id/imageView9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/top"
            android:layout_marginBottom="15dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            android:alpha="1"
            android:layout_marginStart="70dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/top" />
        <Constraint
            android:id="@+id/imageView9"
            app:layout_constraintEnd_toStartOf="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/imageView10"
            android:layout_marginEnd="24dp"
            android:alpha="1"
            app:layout_constraintTop_toTopOf="@+id/imageView10" />
        <Constraint
            android:id="@+id/imageView10"
            app:layout_constraintEnd_toStartOf="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/imageView8"
            android:layout_marginEnd="24dp"
            android:alpha="1"
            app:layout_constraintTop_toTopOf="@+id/imageView8" />
        <Constraint
            android:id="@+id/imageView8"
            app:layout_constraintEnd_toEndOf="@+id/top"
            android:layout_width="26dp"
            android:layout_height="25dp"
            app:layout_constraintBottom_toBottomOf="@+id/top"
            android:layout_marginEnd="32dp"
            android:alpha="1"
            app:layout_constraintTop_toTopOf="@+id/top"
            app:layout_editor_absoluteY="726dp" />
        <Constraint
            android:id="@+id/progressBar"
            app:layout_constraintEnd_toEndOf="@+id/top"
            android:layout_width="0dp"
            android:layout_height="4dp"
            app:layout_constraintTop_toTopOf="@id/top"
            app:layout_constraintStart_toStartOf="@+id/top" />
        <Constraint
            android:id="@+id/playerSong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="28dp"
            app:layout_constraintTop_toTopOf="@+id/songie"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:layout_marginTop="40dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerArtist"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/playerSong"
            android:layout_marginStart="28dp"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:layout_marginTop="8dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerMore"
            app:layout_constraintEnd_toEndOf="@+id/songie"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginEnd="32dp"
            app:layout_constraintTop_toTopOf="@+id/songie"
            android:layout_marginTop="40dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/seekBar"
            app:layout_constraintEnd_toStartOf="@+id/playerDura"
            app:layout_constraintStart_toEndOf="@+id/playerDuration"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toTopOf="@+id/songie"
            android:layout_marginTop="208dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerDuration"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/playerArtist"
            android:layout_marginStart="28dp"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:layout_marginTop="109dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerDura"
            app:layout_constraintEnd_toEndOf="@+id/songie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            app:layout_constraintTop_toBottomOf="@+id/playerMore"
            android:layout_marginTop="136dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerBack"
            android:layout_width="48dp"
            android:layout_height="48dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBar"
            android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="@+id/songie"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerPause"
            app:layout_constraintEnd_toStartOf="@+id/playerNext"
            app:layout_constraintStart_toEndOf="@+id/playerBack"
            android:layout_width="48dp"
            android:layout_height="48dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBar"
            android:alpha="0" />
        <Constraint
            android:id="@+id/playerNext"
            app:layout_constraintEnd_toEndOf="@+id/songie"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginEnd="80dp"
            app:layout_constraintTop_toBottomOf="@+id/seekBar"
            android:alpha="0" />
    </ConstraintSet>

    <Transition
        app:constraintSetEnd="@id/start"
        app:constraintSetStart="@+id/end"
        app:duration="100"
        app:motionInterpolator="easeInOut">
        <OnSwipe
            app:touchAnchorId="@+id/top"
            app:dragDirection="dragUp"
            app:touchAnchorSide="top"
            app:maxAcceleration="1"
            app:maxVelocity="1"
            app:springDamping="100.0" />
        <KeyFrameSet >
            <KeyAttribute
                app:motionTarget="@+id/textView12"
                app:framePosition="6"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/textView14"
                app:framePosition="6"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/imageView8"
                app:framePosition="10"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/imageView9"
                app:framePosition="10"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/imageView10"
                app:framePosition="10"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/backArrow"
                app:framePosition="20"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/backArrow"
                app:framePosition="40"
                android:alpha="0.5" />
            <KeyAttribute
                app:motionTarget="@+id/playerSong"
                app:framePosition="55"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/playerArtist"
                app:framePosition="55"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/playerMore"
                app:framePosition="55"
                android:alpha="0" />
            <KeyAttribute
                app:motionTarget="@+id/seekBar"
                app:framePosition="6"
                android:alpha="0" />
        </KeyFrameSet>
    </Transition>
</MotionScene>
